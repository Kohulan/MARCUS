# Session Management Configuration for MARCUS
# Add these variables to your .env file

# Maximum number of concurrent users (default: 3)
MARCUS_MAX_CONCURRENT_USERS=3

# Session timeout in seconds (default: 3600 = 1 hour)
MARCUS_SESSION_TIMEOUT=3600

# WebSocket heartbeat interval in seconds (default: 30)
MARCUS_HEARTBEAT_INTERVAL=30

# Enable session management (set to false to disable)
MARCUS_ENABLE_SESSION_MANAGEMENT=true

# Backend URL for frontend (adjust for your deployment)
VUE_APP_BACKEND_URL=http://localhost:9000

# Enable debug logging for session management
MARCUS_SESSION_DEBUG=false
